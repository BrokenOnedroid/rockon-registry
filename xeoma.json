{
  "Xeoma Video Surveillance": {
    "containers": {
      "xeoma": {
        "image": "coppit/xeoma",
        "launch_order": 1,
        "ports": {
          "8090": {
            "description": "Xeoma server port",
            "host_default": 8090,
            "label": "server port",
            "protocol": "tcp"
          }
        },
        "volumes": {
          "/config": {
            "description": "Xeoma config path",
            "label": "xeoma config"
          },
          "/archive": {
            "description": "Xeoma archive path",
            "label": "xeoma archive"
          }
        }
      }
    },
    "description": "Xeoma Video Surveillance.<p>Based on a custom docker image: <a href='https://hub.docker.com/r/coppit/xeoma' target='_blank'>https://hub.docker.com/r/coppit/xeoma</a>, available for amd64 architecture only.</p>",
    "volume_add_support": true,
    "website": "https://felenasoft.com/xeoma/en/",
    "version": "1.1"
  }
}
