{
    "Gotify": {
        "description": "A simple server for sending and receiving messages. <p>Based on the official Gotify Docker image: <a href='https://hub.docker.com/r/gotify/server' target='_blank'>https://hub.docker.com/r/gotify/server</a> available for x86_64 architecture only.</p>",
        "ui": {
            "slug": ""
        },
        "website": "https://gotify.net/",
        "version": "latest",
        "containers": {
            "gotify": {
                "image": "gotify/server",
                "launch_order": 1,
                "ports": {
                    "80": {
                        "description": "Gotify server port. Suggested default: 8100",
                        "host_default": 8100,
                        "label": "Server port",
                        "protocol": "tcp",
                        "ui": true
                    }
                },
                "volumes": {
                    "/app/data": {
                        "description": "Choose a Share for the database, plugins and images for applications.  Eg: create a Share called gotify for this purpose.",
                        "label": "Database and App Images",
                        "min_size": 1073741824
                    }
                },
                "environment": {
                    "TZ": {
                        "label": "Timezone",
                        "description": "Example: Europe/Berlin"
                    }
                }
            }
        },
        "more_info": "<p>Default username is <code>admin</code> and password is <code>admin</code>. For usage instructions visit <a href=\"https://gotify.net/docs/first-login\" target=\"_blank\">the gotify Getting Started guide</a></p>"
    }
}
